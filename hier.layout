# Select a hierarchy sceme using:  ./configure --enable-hier=xs

[xs]
rootdir=	$(prefix)/lib/httpd
configdir=	conf
contribdir=	contrib
logdir=		logs
rundir=		/var/run
moduledir=	module

[gnu]
rootdir=	$(datarootdir)/xs-httpd
configdir=	$(sysconfdir)/xs-httpd
contribdir=	contrib
logdir=		$(localstatedir)/log/xs-httpd
rundir=		$(localstatedir)/run
moduledir=	$(libexecdir)/xs-httpd

